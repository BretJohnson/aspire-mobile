<Project>
    <ItemGroup>
        <AppStubProjectFiles Include="$(MSBuildThisFileDirectory)\..\contentFiles\**\*.*" />
    </ItemGroup>
    <Target Name="CopyAppStubProjectFiles" BeforeTargets="Build">
        <Copy SourceFiles="@(AppStubProjectFiles)" DestinationFolder="$(TargetDir)\%(RecursiveDir)" />
    </Target>
</Project>
